/**
 * Represents well known icon font classes for reuse.
 */
export abstract class ZWellKnownIconClasses {
  /**
   * Font Awesome Classic.
   *
   * @see {@link https://fontawesome.com}
   */
  public static readonly FontAwesome = 'https://fontawesome.com';

  /**
   * Font Awesome Brands.
   *
   * @see {@link https://fontawesome.com/search?o=r&f=brands}
   */
  public static readonly FontAwesomeBrands = 'https://fontawesome.com/search?o=r&f=brands';

  /**
   * Font Awesome Sharp.
   *
   * @see {@link https://fontawesome.com/search?o=r&f=sharp}
   */
  public static readonly FontAwesomeSharp = 'https://fontawesome.com/search?o=r&f=sharp';

  /**
   * Material design.
   *
   * @see {@link https://mui.com/material-ui/material-icons}
   */
  public static readonly Material = 'https://mui.com/material-ui/material-icons';
}
